{
  "name": "@bygdle/search-js",
  "version": "0.3.0",
  "description": "simple search and filter functions",
  "main": "lib",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "scripts": {
    "prepare": "npm run build && npx ts-patch install",
    "test": "jest src",
