    "@types/node": "^18.7.18",
    "@types/prompt-sync": "^4.2.0",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "depcheck": "^1.4.3",
    "esbuild": "^0.15.10",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.28.0",
    "jest": "^29.0.3",
    "klaw-sync": "^6.0.0",
